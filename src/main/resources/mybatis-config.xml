    <mappers>
        <mapper resource="mappers/board-mapper.xml"/>
        <mapper resource="mappers/team-mapper.xml"/>
        <mapper resource="mappers/member-mapper.xml"/>
        <mapper resource="mappers/admin-mapper.xml"/>
        <mapper resource="mappers/match-mapper.xml"/>
        <mapper resource="mappers/stadium-mapper.xml"/>
        <mapper resource="mappers/attachment-mapper.xml"/>
