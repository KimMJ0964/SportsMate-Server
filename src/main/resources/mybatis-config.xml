        <typeAlias type="com.kh.sportsmate.match.model.vo.MatchQna" alias="MatchQna"/>
        <typeAlias type="com.kh.sportsmate.match.model.dto.MyMatch" alias="MyMatch"/>

        <!-- Attachment -->
        <typeAlias type="com.kh.sportsmate.Attachment.model.vo.Profile" alias="Profile"/>
